<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>GTK# Backend for Xamarin.Forms</Description>
    <AssemblyName>Microsoft.Maui.Controls.Compatibility.GTK</AssemblyName>
    <TargetFrameworks>net472</TargetFrameworks>
    <LangVersion>9.0</LangVersion>
  </PropertyGroup>
  <PropertyGroup>
    <RootNamespace>Microsoft.Maui.Controls.Compatibility.GTK</RootNamespace>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblySearchPaths>$(AssemblySearchPaths);{GAC}</AssemblySearchPaths>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="gtk-sharp" Version="2.12.0.0"/>
    <Reference Include="glib-sharp" Version="2.12.0.0"/>
    <Reference Include="atk-sharp" Version="2.12.0.0"/>
    <Reference Include="gdk-sharp" Version="2.12.0.0"/>
    <Reference Include="pango-sharp" Version="2.12.0.0"/>
    <Reference Include="Mono.Cairo" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="..\..\..\..\Controls\src\Core\Crc64.cs" Link="Crc64.cs"/>
    <Compile Include="..\..\..\..\Controls\src\Core\StreamWrapper.cs" Link="StreamWrapper.cs"/>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="OpenTK" Version="3.0.1"/>
    <PackageReference Include="Microsoft.Maui.Graphics"/>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\..\..\Controls\src\Core\Controls.Core.csproj"/>
    <ProjectReference Include="..\..\..\..\Core\src\Core.csproj"/>
  </ItemGroup>
